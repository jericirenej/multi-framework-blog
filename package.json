{
  "name": "multi-framework-blog",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "server": "pnpm run --filter '@/api' serve",
    "seed": "pnpm run --filter '@/db' seed",
    "migrate:rollback": "pnpm --filter '@/db' migrate:rollback",
    "migrate": "pnpm --filter '@/db' migrate",
    "check": "pnpm --filter '*' run check",
    "lint": "pnpm --filter '*' run lint",
    "test": "pnpm --filter '*' run test",
    "storybook:angular": "pnpm --filter 'angular' run storybook"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "^7.0.0",
    "vitest": "catalog:",
    "zx": "catalog:"
  },
  "dependencies": {
    "@/api": "workspace:*",
    "@/db": "workspace:*"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
